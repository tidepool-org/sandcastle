{
  "name": "sandcastle",
  "version": "0.0.0",
  "description": "Tidepool Project's upload tracking server.",
  "main": "server.js",
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tidepool-org/sandcastle.git"
  },
  "keywords": [
    "git",
    "upload",
    "tracker"
  ],
  "config": {
    "blanket": {
      "data-cover-never": [ "node_modules" ],
      "pattern": [
        "lib/upload",
        "server",
        "env"
      ]
    }
  },
  "author": "Ben West",
  "license": "BSD-2",
  "bugs": {
    "url": "https://github.com/tidepool-org/sandcastle/issues"
  },
  "homepage": "https://github.com/tidepool-org/sandcastle",
  "dependencies": {
    "restify-git-json": "0.0.4",
    "hakken": "0.0.9",
    "nurlize": "~0.1.5"
  },
  "devDependencies": {
    "should": "~3.1.0",
    "request": "~2.33.0",
    "mocha": "~1.17.1",
    "event-stream": "~3.1.0",
    "travis-cov": "~0.2.4",
    "coveralls": "~2.6.1",
    "mocha-lcov-reporter": "0.0.1",
    "blanket": "~1.1.6"
  }
}
